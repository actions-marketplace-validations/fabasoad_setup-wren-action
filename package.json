{
  "name": "setup-wren-action",
  "version": "1.0.0",
  "description": "This GitHub action installs wren",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-wren-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint": "eslint --ext ts src",
    "test": "snyk test && jest --config=jest.config.json --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "actions"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-wren-action#readme",
  "dependencies": {
    "@actions/core": "1.2.6"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "@zeit/ncc": "0.22.3",
    "eslint": "7.13.0",
    "eslint-config-google": "0.14.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "snyk": "1.424.4",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "snyk": true
}
